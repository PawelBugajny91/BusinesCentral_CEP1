package com.myspace.cep_1;

import java.lang.Number;

rule "PartialPredictedBlinding"
	dialect "mvel"
	activation-group "cep"
	salience 3
	when
		ares : AresEvent( ) over window:time (2s)
		( FDEvent( this during ares ) over window:time (2s) or FDEvent( this overlaps ares ) over window:time (2s) or FDEvent( this overlappedby ares ) over window:time (2s) )
	then
		AlertEvent fact0 = new AlertEvent();
		fact0.setDuration( ares.duration );
		fact0.setTime( ares.time );
		fact0.setStatus( "partial predicted blinding" );
		insertLogical( fact0 );
end
