package com.myspace.cep_1;

import java.lang.Number;

rule "AresUpdate"
	dialect "mvel"
	activation-group "ares"
	salience 3
	when
		ares : AresEvent( ) over window:time (10)
		asa : ASA1311Z( this after [0 2m] ares ) over window:time (10)
	then
		modify( ares ) {
				setDuration( asa.time-ares.time )
		}
end
