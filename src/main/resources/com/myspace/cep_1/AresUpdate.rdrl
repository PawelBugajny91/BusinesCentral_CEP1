package com.myspace.cep_1;

import java.lang.Number;

rule "AresUpdate"
	dialect "mvel"
	activation-group "ares"
	salience 3
	no-loop true
	when
		ares : AresEvent( ) over window:time (2m)
		asa : ASA1311Z( this after [0s, 2m] ares , value == 1 ) over window:time (2m)
	then
		AresEvent fact0 = new AresEvent();
		fact0.setTime( ares.time );
		fact0.setDuration( asa.time-ares.time );
		insert( fact0 );
		retract( ares );
end
