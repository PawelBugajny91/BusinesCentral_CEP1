package com.myspace.cep_1;

import java.lang.Number;

rule "AresUpdate"
	dialect "mvel"
	activation-group "ares"
	salience 3
	when
		asa : ASA1311Z( value == 1 ) over window:time (65s)
		ares : AresEvent( this before asa ) over window:time (1m)
	then
		modify( ares ) {
				setDuration( asa.time - ares.time )
		}
end
