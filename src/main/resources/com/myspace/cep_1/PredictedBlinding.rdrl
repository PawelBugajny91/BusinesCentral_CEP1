package com.myspace.cep_1;

import java.lang.Number;

rule "PredictedBlinding"
	dialect "mvel"
	activation-group "cep"
	salience 2
	when
		ares : AresEvent( ) over window:time (2s)
		fd : FDEvent( this includes ares ) over window:time (2s)
	then
		AlertEvent fact0 = new AlertEvent();
		fact0.setDuration( ares.duration );
		fact0.setTime( ares.time );
		fact0.setStatus( "predicted blinding" );
		insertLogical( fact0 );
end
