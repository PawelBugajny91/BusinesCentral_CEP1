package com.myspace.cep_1;

import java.lang.Number;

rule "Blind"
	dialect "mvel"
	activation-group "cep"
	salience 5
	when
		ares : AresEvent( ) over window:time (2s)
	then
		Alert fact0 = new Alert();
		fact0.setTime( ares.time );
		fact0.setDuration( ares.duration );
		fact0.setStatus( blinding );
		insertLogical( fact0 );
end
