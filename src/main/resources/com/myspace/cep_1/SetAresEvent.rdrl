package com.myspace.cep_1;

import java.lang.Number;

rule "SetAresEvent"
	dialect "mvel"
	activation-group "ares"
	salience 1
	no-loop true
	when
		asa : ASA1311Z( value == 1 ) over window:time (2m)
	then
		AresEvent fact0 = new AresEvent();
		fact0.setTime( asa.time );
		fact0.setDuration( 35 );
		insert( fact0 );
		AlertEvent fact1 = new AlertEvent();
		fact1.setTime( asa.time-15 );
		fact1.setDuration( 69 );
		insertLogical( fact1 );
end
